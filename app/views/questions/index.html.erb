<h2>新規投稿</h2>

 <%= render "form" %>

<%# 質問一覧を降順表示 %>
<table>
  <tbody>
    <% @questions.each.with_index(1) do |question, i| %>
      <tr>
        <td>
        <button type="button" class="btn btn-outline-light btn-lg btn-block"><%= link_to question.title, question %></button>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>